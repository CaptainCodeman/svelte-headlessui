{
  "name": "svelte-headlessui",
  "description": "HeadlessUI components for Svelte",
  "version": "0.0.18",
  "type": "module",
  "keywords": [
    "svelte",
    "component",
    "headless",
    "ui",
    "tailwindcss",
    "tailwindui",
    "renderless"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://captaincodeman.github.io/svelte-headlessui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/captaincodeman/svelte-headlessui.git"
  },
  "author": {
    "name": "Simon Green",
    "email": "simon@captaincodeman.com",
    "url": "https://www.captaincodeman.com/"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "test": "svelte-kit sync && playwright test",
    "package": "svelte-kit sync && tsup && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives --ignore-path ./.gitignore \"**/*.{js,cjs,mjs,ts,cts,mts,svelte}\"",
    "sourcemap": "source-map-explorer dist/*.js"
  },
  "tsup": {
    "entry": [
      "src/lib/index.ts"
    ],
    "format": [
      "esm"
    ],
    "external": [
      "svelte/store"
    ],
    "sourcemap": true,
    "minify": true,
    "clean": true,
    "dts": true
  },
  "devDependencies": {
    "@playwright/experimental-ct-svelte": "^1.35.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.20.4",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "eslint-plugin-svelte": "^2.31.0",
    "happy-dom": "^9.20.3",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "publint": "^0.1.12",
    "source-map-explorer": "^2.5.3",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.4",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.3",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "svelte-transition": "^0.0.10"
  }
}
