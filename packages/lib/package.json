{
  "name": "svelte-headlessui",
  "description": "HeadlessUI components for Svelte",
  "version": "0.0.9",
  "type": "module",
  "keywords": [
    "svelte",
    "component",
    "headless",
    "ui",
    "tailwindcss",
    "tailwindui",
    "renderless"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://captaincodeman.github.io/svelte-headlessui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/captaincodeman/svelte-headlessui.git"
  },
  "author": {
    "name": "Simon Green",
    "email": "simon@captaincodeman.com",
    "url": "https://www.captaincodeman.com/"
  },
  "scripts": {
    "build": "svelte-kit sync && tsup",
    "dev": "vite dev",
    "test": "vitest run src",
    "test:watch": "vitest src",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives --ignore-path ./.gitignore \"**/*.{js,cjs,mjs,ts,cts,mts,svelte}\"",
    "sourcemap": "source-map-explorer dist/*.js"
  },
  "tsup": {
    "entry": [
      "src/lib/index.ts"
    ],
    "format": [
      "esm"
    ],
    "external": [
      "svelte/store"
    ],
    "sourcemap": true,
    "minify": true,
    "clean": true,
    "dts": true
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.0",
    "@sveltejs/kit": "1.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.30.0",
    "eslint-plugin-svelte": "^2.14.1",
    "happy-dom": "^8.1.0",
    "postcss": "^8.4.20",
    "postcss-load-config": "^4.0.1",
    "source-map-explorer": "^2.5.3",
    "svelte": "^3.55.0",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vitest": "^0.25.8"
  },
  "dependencies": {
    "svelte-transition": "^0.0.7"
  }
}
